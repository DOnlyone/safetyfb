.toTop{
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
  margin: 0 0 50rpx 50rpx;
  border-radius: 50%;
  color: #ffffff;
  font-size: 36rpx;
  line-height: 80rpx;
  text-align: center;
  background: #f65758;
}
.item-header{
  color: #f65357;
  font-size:36rpx;
  margin: 2rpx 0;
}

 .item-header .header-round{
    width: 40rpx;
    height: 40rpx;
    margin: 0 48rpx 0 0;
    vertical-align: middle;
 }

 .item-header text{
    vertical-align: middle;
 }


.item-body{
  margin: 0 30rpx 0 20rpx;
  border-left: #6f8392 solid 4rpx;
  padding: 20rpx 0 40rpx 0;
}

.change-item{
    margin-left: 68rpx;
    font-size: 30rpx;
    color: #a4b6c2;
    line-height: 1.5;
}

